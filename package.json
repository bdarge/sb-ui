    "@angular/material": "18.2.14",
    "@angular/platform-browser": "^19.1.4",
    "@angular/platform-browser-dynamic": "19.1.4",
    "@angular/router": "19.1.4",
    "@babel/runtime": "7.23.9",
    "@cypress/schematic": "2.5.1",
    "@fortawesome/angular-fontawesome": "0.15.0",

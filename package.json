{
  "name": "ui",
  "version": "0.0.2",
  "description": "Angular 18 (Typescript + SASS + Hot Reload)",
  "author": {
    "name": "bdarge",
    "email": "bin.darge@gmail.com"
  },
  "packageManager": "pnpm@10.5.2",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular 19",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "pnpm dlx rimraf ./dist",
    "config": "node -r ts-node/register --no-warnings --loader ts-node/esm set-env.ts",
    "ng": "ng",
    "start": "pnpm run config && ng serve ",
    "build": "pnpm run config && ng build",
    "test": "pnpm run config && pnpm run lint && ng test --configuration=test --browsers=ChromeHeadlessCustom",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e",
    "ci": "pnpm run clean && pnpm lint && ng test --configuration=test --browsers ChromeHeadlessCustom --code-coverage && pnpm run build:prod --deploy-url / --base-href /",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "eslint --color -c eslintrc.json --ext .ts .",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1902.0",
    "@angular-devkit/build-angular": "19.2.0",
    "@angular-eslint/builder": "16.3.1",
